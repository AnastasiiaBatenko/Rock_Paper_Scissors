    def display_scores(self):
        print("\nFinal Scores:")
        for player, score in self.tic_tac_toe.scores.items():
            print(f"{player}: {score}")
        winner = max(self.tic_tac_toe.scores, key=self.tic_tac_toe.scores.get)
        print(f"Overall winner: {winner}!")
